{
  "name": "@jamesgopsill/ergast-f1-ts-client",
  "version": "0.0.1",
  "description": "A Typescript Client for the Formula One API.",
  "keywords": [
    "Formula One"
  ],
  "author": "James Gopsill",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesgopsill/ergast-f1-ts-client."
  },
  "bugs": {
    "url": "https://github.com/jamesgopsill/ergast-f1-ts-client./issues"
  },
  "homepage": "https://jamesgopsill.github.io/ergast-f1-ts-client.",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:docs": "typedoc",
    "example": "ts-node examples/drivers.ts",
    "format": "prettier --write src/**/* && prettier --write __tests__/**/* && prettier --write examples/**/*",
    "all": "pnpm format && pnpm test && pnpm build && pnpm build:docs"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.11",
    "typescript": "^4.8.2"
  }
}
